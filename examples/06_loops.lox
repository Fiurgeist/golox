print "while:";
var x = 5;
while (x > 0) {
  print x;
  x = x - 1;
}

print "for:";
for (var i = 0; i < 5; i = i + 1) {
  print i;
}

print "for(;c;):";
for (; x < 5;) {
  print x;
  x = x + 1;
}

print "Fibonacci:";
var a = 0;
var temp;
for (var b = 1; a < 10000; b = temp + b) {
  print a;
  temp = a;
  a = b;
}
